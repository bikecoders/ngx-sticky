{
  "name": "ngx-sticky-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy-gh-pages": "npm run build:prod -- --base-href 'https://bikecoders.github.io/ngx-sticky/' && angular-cli-ghpages --repo=https://GH_TOKEN@github.com/bikecoders/ngx-sticky.git --dir=dist/ngx-sticky-demo --branch=gh-pages --name='bikecoders' --email='bikecoders@gmail.com'",
    "sticky:build": "ng build sticky-directive",
    "sticky:build-prod": "ng build sticky-directive --prod",
    "sticky:publish": "npm run sticky:build-prod && npm run sticky:publish:copy-readme && npm run sticky:publish:copy-licence && cd dist/sticky-directive && npm publish",
    "sticky:publish:copy-licence": "copy LICENSE dist/sticky-directive",
    "sticky:publish:copy-readme": "copy README.md dist/sticky-directive",
    "sticky:publish:copy-files": "npm run sticky:publish:copy-licence && npm run sticky:publish:copy-readme",
    "sticky:lint": "ng lint sticky-directive",
    "sticky:lint:report": "npm --silent run sticky:lint -- --format json | sed -n '1!p' > reports/lint_issues.json",
    "sticky:test": "ng test sticky-directive",
    "sticky:test:coverage": "ng test sticky-directive -- --watch=false --code-coverage --browsers=ChromeHeadless",
    "sticky:test:sonar": "npm run sticky:lint:report && cross-env SONAR_QUBE=1 npm run sticky:test:coverage && sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.5.0",
    "copy": "^0.3.2",
    "cross-env": "^5.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sonarqube-unit-reporter": "^0.0.18",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
